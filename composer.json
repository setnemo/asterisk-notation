{
    "name": "setnemo/asterisk-notation",
    "description": "Asterisk notation for array access in PHP",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "setnemo",
            "email": "setnemo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|~8.0.0",
        "ext-json": "*",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "vimeo/psalm": "^4.7",
        "squizlabs/php_codesniffer": "*",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Setnemo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit",
            "Tests\\Helper\\": "tests/_support/Helper"
        }
    }
}
